<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="model_file" default="$(find-pkg-share dallara_description)/urdf/vehicle.xacro" description="path to the file of model settings (*.xacro)"/>
    <node name="robot_state_publisher" pkg="robot_state_publisher" exec="robot_state_publisher">
      <param name="robot_description" value="$(command 'xacro $(var model_file)' 'warn')"/>
    </node>
</launch>
